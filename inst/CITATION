citHeader("To cite LendingClub in publications, please use:")

year <- sub("-.*", "", meta$Date) 
note <- sprintf("R package version %s", meta$Version)  


bibentry(bibtype = "Manual",
         title = "LendingClub: A Lending Club API Wrapper.",
         author = person("Ryan", "Kuhn"),
         year = year,
         note = note,
         url = "https://CRAN.R-project.org/package=LendingClub" )
